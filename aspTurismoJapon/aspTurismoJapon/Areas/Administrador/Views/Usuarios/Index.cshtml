                    <td>@item.Contrasena</td>
                    <td>@item.Rol</td>
                    <td>
                        <table>
                            <td>
                                <a href="~/Administrador/Usuarios/Editar/@item.Id">Editar</a>

                            </td>
                            <td>
                                <a href="~/Administrador/Usuarios/Eliminar/@item.Id">Eliminar</a>
                            </td>
                        </table>
                    </td>
                </tr>
                x++;
            }
