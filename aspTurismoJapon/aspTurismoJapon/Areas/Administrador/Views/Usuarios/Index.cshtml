@model IEnumerable<aspTurismoJapon.Models.Usuarios>
@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
    int x = 1;
}

<section class="class_acciones">
    <table>
        <tbody>
        <td>
            <h1>Administrador - Usuarios</h1>
        </td>
        <td>
            <a href="~/Administrador/Usuarios/Agregar">Agregar nuevo Usuario</a>
        </td>
        </tbody>
    </table>
